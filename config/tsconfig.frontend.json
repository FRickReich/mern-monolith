{
    "compilerOptions": {
      "target": "es6",
      "jsx": "react-jsx",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "strictNullChecks": false,
      "forceConsistentCasingInFileNames": true,
      "outDir": "../build/frontend"
    },
    "include": ["../src/frontend/**/*"]
  }