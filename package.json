{
  "name": "monolith-2",
  "version": "1.1.2",
  "description": "Fullstack Application with React frontend and Express.js backend using TypeScript.",
  "main": "index.js",
  "scripts": {
    "clean:install": "rimraf ./node_modules/ && rm ./yarn.lock",
    "clean:build": "rimraf ./build && rimraf ./public/bundle.js",
    "clean:docs": "rimraf ./docs",
    "clean:coverage": "rimraf ./coverage",
    "clean": "yarn run clean:build && yarn run clean:docs && yarn run clean:coverage && yarn run clean:install",
    "build:frontend": "esbuild src/frontend/index.tsx --bundle --outfile=public/bundle.js --tsconfig=./config/tsconfig.frontend.json",
    "build:backend": "tsc --project ./config/tsconfig.backend.json",
    "build:prepare": "npm run format && npm run lint && npm run test",
    "build:storybook": "storybook build",
    "build": "yarn run clean:build && yarn run build:frontend && yarn run build:backend",
    "watch:frontend": "esbuild src/frontend/index.tsx --bundle --outfile=public/bundle.js --tsconfig=./config/tsconfig.frontend.json --watch",
    "watch:backend": "nodemon --watch src/backend --exec ts-node src/backend/server.ts",
    "watch": "concurrently \"yarn run watch:frontend\" \"yarn run watch:backend\"",
    "start": "node build/backend/server.js",
    "start:dev": "NODE_ENV=development yarn run watch",
    "start:prod": "NODE_ENV=production yarn run start",
    "lint:backend": "eslint -c ./config/.eslintrc.backend.js 'src/backend/**/*.{ts,tsx}'",
    "lint:frontend": "eslint -c ./config/.eslintrc.frontend.js 'src/frontend/**/*.{ts,tsx}'",
    "lint": "yarn run lint:backend && yarn run lint:frontend",
    "lint:fix": "yarn run lint:backend --fix && yarn run lint:frontend --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "test:frontend": "jest --config ./config/jest.frontend.config.js",
    "test:backend": "jest --config ./config/jest.backend.config.js",
    "test": "yarn run test:frontend && yarn run test:backend",
    "coverage:frontend": "jest --config ./config/jest.frontend.config.js --coverage",
    "coverage:backend": "jest --config ./config/jest.backend.config.js --coverage",
    "coverage": "yarn run clean:coverage && npm run coverage:frontend && npm run coverage:backend",
    "type-check:frontend": "tsc --noEmit --project ./config/tsconfig.frontend.json",
    "type-check:backend": "tsc --noEmit --project ./config/tsconfig.backend.json",
    "type-check": "yarn run type-check:frontend && yarn run type-check:backend",
    "build:ci": "yarn run lint && yarn run type-check && yarn run test && yarn run build",
    "docs:frontend": "typedoc --out ./docs/frontend ./src/frontend/index.tsx --tsconfig ./config/tsconfig.frontend.json",
    "docs:backend": "typedoc --out ./docs/backend ./src/backend/server.ts --tsconfig ./config/tsconfig.backend.json",
    "docs": "yarn run clean:docs && yarn run docs:frontend && yarn run docs:backend",
    "commit": "git-cz",
    "release": "standard-version",
    "storybook": "storybook dev -p 6006"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-webpack5": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "standard-version": "^9.5.0",
    "storybook": "^8.1.11",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
